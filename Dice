import random

min = 1
max = 6

roll_again = 'yes'

while roll_again == 'yes' or roll_again == 'y':
  print('Rolling the Dice......')
  print(random.randint(min,max))
  print(random.randint(min,max))
  roll_again = input('Roll Again?')
while roll_again == 'no' or roll_again == 'n':
  roll_again == 'n'
  print('Goodbye')
  break



